<%= render "admin/shared/links" %>
  <div class="container">
    <div class="mt-5">
      <h2 class="bg-light col-3 text-center">
        社員一覧
      </h2>
    </div>
    <div class="text-center">
      <table class="table">
        <tbody>
          <% @enployees.each do |enployee| %>
            <tr>
              <td>
                <%= link_to admin_enployee_path(enployee.id) do %>
                  <%= enployee.last_name %> <%= enployee.first_name %>
                <% end %>
              </td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
